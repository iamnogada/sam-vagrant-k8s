- name: install required for docker
  yum:  name=docker-ce-selinux-17.03.2.ce-1.el7.centos.noarch state=present

- name: install docker
  yum:  name=docker-ce-17.03.2.ce-1.el7.centos.x86_64 state=present

- name: register docker as a service 
  command: systemctl enable docker

- name: register docker as a service 
  command: systemctl start docker

- name: setting cgroup for docker daemon
  copy: src=daemon.json dest=/etc/docker/daemon.json


